<configuration>

    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>[%d] %-5level %msg \(%c\)%n</pattern>
        </encoder>
    </appender>

    <root level="${LOGGING_ROOT_LEVEL:-INFO}">
        <appender-ref ref="STDOUT" />
    </root>

</configuration>